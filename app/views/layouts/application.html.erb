      <p><%= link_to image_tag('facebook.png', :alt => 'itagui', :width=>"100", :height=>"50"), "https://es-la.facebook.com/fiestaycumbia/" %></p>   
      
    </header>
    <main role="main">
       <%= render 'layouts/messages' %>
      <div  class="container">
       <%= yield %>
      </div>
    </main>
  </body>
</html>